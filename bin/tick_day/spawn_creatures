#!/usr/bin/env coffee

_ = require 'underscore'
async = require 'async'
db = require '../../db'
data = require '../../data'
commands = require '../../commands'

fin = (err) ->
  if err?
    console.log err
    process.exit 1
  else
    console.log 'Animals Spawned'
    process.exit 0

fin()

query =
  creature: {$exists: true}
db.characters.find(query).toArray (err, all_creatures) ->
  return fin(err) if err?
  fin()
